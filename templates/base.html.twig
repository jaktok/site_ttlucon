					<div class="dropdown-menu" aria-labelledby="navbarDropdown">
						<a class="dropdown-item" href="{{ path ('individuel', {'cat': cat} ) }}">Resultats</a> <a
							class="dropdown-item" href="/calendrier">Calendrier</a>
