					{% set cat = 'Adulte' %}
					<div class="dropdown-menu" aria-labelledby="navbarDropdown">
						{% set cat = 'Adulte' %}
						<a class="dropdown-item" href="{{ path ('equipe', {'cat': cat} ) }}">Equipes</a>
