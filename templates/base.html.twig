						<a class="dropdown-item" href="{{ path ('partenaire') }}">Partenaires</a>
						<div class="dropdown-divider"></div>
						{% set cat = 'tous' %}
						<a class="dropdown-item" href="{{ path ('joueur', {'cat': cat} ) }}">Liste des joueurs</a>
