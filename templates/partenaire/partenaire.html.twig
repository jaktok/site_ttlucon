{% extends 'base.html.twig' %} {% block title %} Partenaires{% endblock
%} {% block body %}
<meta charset="UTF-8" />
<style>
</style>


<div class="container">



	{% if partenaires is defined %} {% for partenaire in partenaires %} {%
	set nomFichier = partenaire.nomPhoto %}
	<div class="row mt-4">
		<div class="col-sm-4 d-flex justify-content-center align-items-center" style="font-family: 'Trebuchet MS', sans-serif;;font-style: italic;" >
			<h2>{{ partenaire.titre }}</h2>
		</div>
		<div class="col-sm-8 d-flex justify-content-center align-items-center" >
			<div class="col-sm-9">
				<img alt="" src="{{ asset('partenaires/'~nomFichier) }}" alt=""
					class="img-fluid"  width="25%">
			</div>
		</div>
	</div>
	<div class="row mt-5">
		<div class="col">
			<!-- <hr color="black" >-->
		</div>
	</div>
	<div class="row">
		<div class="col-sm-8">{{ partenaire.texte|raw }}</div>
	</div>
	<div class="divider bg-dark mt-4"><hr></div>
	{% endfor %} {% endif %} 
</div>
	
	{% endblock %}