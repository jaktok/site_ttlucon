{% extends 'parametrage/base_parametrage.html.twig' %} {% block title
%}Matchs joueur compet{% endblock %}

{% block body %}
<meta charset="UTF-8" />

<!-- Insérer cette balise "link" après celle de Bootstrap -->
<link rel="stylesheet"
	href="https://unpkg.com/bootstrap-table@1.16.0/dist/bootstrap-table.min.css">

<!-- Insérer cette balise "script" après celle de Bootstrap -->
<script
	src="https://unpkg.com/bootstrap-table@1.16.0/dist/bootstrap-table.min.js"></script>


{{ form_start(formJoueurMatch) }}

<div class="container">
	<h1>Matchs Joueur</h1>


	
	<div class="row">
		<div class="col-sm-3" >
			{{ form_row(formJoueurMatch.joueur_compet) }}
		</div>
		<div class="col-sm-2">
			{{ form_row(formJoueurMatch.victoires) }}
		</div>
		<div class="col-sm-2">
			{{ form_row(formJoueurMatch.defaites) }}
		</div>
		<div class="col-sm-2">
			{{ form_row(formJoueurMatch.position) }}
		</div>	
	</div>

<br><br>
	<div class="row">	
		<div class="col-sm-2">
		<button type="submit" class="btn btn-success">Enregistrer</button>
 		</div>
 	<div class="col-sm-2">
 		<a href="{{ path('renseigne_compet', { 'id': idCompet})}}"  class="btn btn-primary" aria-current="page" >Retour</a>
 	</div>
 	</div>
</div>


{{ form_end(formJoueurMatch) }} {% endblock %}


